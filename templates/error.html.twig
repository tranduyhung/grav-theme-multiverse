{% extends 'partials/base.html.twig' %}

{% block content %}
<div id="wrapper">
    <div id="page">
        <h1>{{ 'ERROR'|t }} {{ page.header.http_response_code }}</h1>
        <p>
            {{ page.content|raw }}
        </p>
        <p>
            <a href="{{ base_url == '' ? '/' : base_url }}" class="special">{{ 'BACK_TO_HOME_PAGE'|t }}</a>
        </p>
    </div>
</div>
{% endblock %}
